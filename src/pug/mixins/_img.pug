mixin image_retina_media(images, ext, alt)
    - 
        type = ext
        img_path_default = img_path
    if type === "jpg"
        - type = "jpeg"     
    picture(class!=attributes.class)&attributes(attributes)    
        each item in images
            if item.media !== '0'
                source(
                    srcset=img_path_default + item.image + "." + ext + " 1x, " + img_path_default + item.image + "@2x." + ext + " 2x"
                    media="(min-width: "+item.media+"px)"
                    type="image/"+type
                )
            else
                source(
                    srcset=img_path_default + item.image + "." + ext + " 1x, " + img_path_default + item.image + "@2x." + ext + " 2x"
                    type="image/"+type
                    )    
        img(
            src=img_path_default + images[images.length - 1].image + "." + ext + " 1x, " + img_path_default + images[images.length - 1].image + "@2x." + ext + " 2x"
            alt=alt
            data-gtm-category!=attributes.gtmCategory
            data-gtm-action!=attributes.gtmAction
            data-gtm-label!=attributes.gtmLabel
            data-gtm-type!=attributes.gtmType
            )
