mixin mx_picture_retina(myclass, imgurl, exten, altt)
    picture
        source(srcset=imgurl+'-desk.'+exten+' 1x, '+imgurl+'-desk@2x.'+exten+' 2x' media='(min-width: 768px)')
        img(class=myclass src=imgurl+'-mob.'+exten srcset=imgurl+'-mob.'+exten+' 1x, '+imgurl+'-mob@2x.'+exten+' 2x' alt=altt title=altt)

mixin mx_picture_retina_extra(myclass, imgurl, exten, altt)
