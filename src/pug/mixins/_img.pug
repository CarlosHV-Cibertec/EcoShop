mixin image_webp_retina(image, ext, alt, path)
    - 
        type = ext
        img_path_default = img_path
    if path === 'empresas'
        - img_path_default = img_empresas_path
    if path === 'personas'
        - img_path_default = img_personas_path
    if type === "png"
        - type = "png"     
    if type === "jpg"
        - type = "jpeg"  
    if path === 'empresas'
        - img_path_default = img_empresas_path

    //-picture(class!=attributes.class)
    picture(class!=attributes.class)&attributes(attributes)       
        if type !== "svg"
            //- if(process.env.NODE_ENV == 'production')
            //-     source(
            //-         srcset="/wp-content/uploads/" + image + ".webp 1x, " + "/wp-content/uploads/" + image + "@2x.webp 2x"
            //-         type="image/webp"
            //-         )
            source(
                srcset=img_path_default + image + "." + ext + " 1x, " + img_path_default + image + "@2x." + ext + " 2x"
                type="image/"+type
                )
        img(
            src=img_path_default + image + "." + ext 
            alt=alt
            class=attributes
            data-gtm-category!=attributes.gtmCategory
            data-gtm-action!=attributes.gtmAction
            data-gtm-label!=attributes.gtmLabel
            data-gtm-type!=attributes.gtmType
            )

