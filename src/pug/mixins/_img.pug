mixin image_webp_retina_media_path(image_mobile, ext, image, media, alt, path)
    - 
        type = ext
        img_path_default = img_path
    if path === 'empresas'
        - img_path_default = img_empresas_path
    if path === 'personas'
        - img_path_default = img_personas_path

    
    if type === "jpg"
        - type = "jpeg"     
    //-picture(class!=attributes.class)
    picture(class!=attributes.class)&attributes(attributes)  
        //- if(process.env.NODE_ENV == 'production')  
        //-     source(
        //-         srcset="/wp-content/uploads/" + image + ".webp 1x, " + "/wp-content/uploads/" + image + "@2x.webp 2x"
        //-         media="(min-width: "+media+"px)"
        //-         type="image/webp"
        //-         )
        source(
            srcset=img_path_default + image + "." + ext + " 1x, " + img_path_default + image + "@2x." + ext + " 2x"
            media="(min-width: "+media+"px)"
            type="image/"+type
        )
        //-source(
            srcset=img_path_default + image_mobile + ".webp 1x, " + img_path_default + image_mobile + "@2x.webp 2x"
            type="image/webp"
            )
        //- source(
        //-     srcset=img_path_default + image_mobile + "." + ext + " 1x, " + img_path_default + image_mobile + "@2x." + ext + " 2x"
        //-     type="image/"+type
        //-     )
        img(
            src=img_path_default + image_mobile + "." + ext 
            srcset=img_path_default + image_mobile + "." + ext + " 1x, " + img_path_default + image_mobile + "@2x." + ext + " 2x"
            alt=alt
            type="image/"+type
            
            )
